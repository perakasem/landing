{
	"name": "landing",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test": "vitest run",
		"test:unit": "vitest run --exclude '**/*.integration.test.ts'",
		"test:integration": "vitest run --grep '\\[INTEGRATION\\]'",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"health-check": "node --loader ts-node/esm scripts/health-check.ts"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.6",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/node": "^22.14.0",
		"@types/tailwindcss": "^3.0.11",
		"@vitest/ui": "^4.0.10",
		"autoprefixer": "^10.4.20",
		"jsdom": "^27.2.0",
		"postcss": "^8.5.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"remark-footnotes": "2.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-stacks-icons": "^0.7.0",
		"tailwindcss": "^4.0.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^4.0.10"
	},
	"dependencies": {
		"@microflash/rehype-figure": "^2.1.4",
		"@microflash/remark-figure-caption": "^2.0.2",
		"@supabase/supabase-js": "^2.83.0",
		"@tailwindcss/cli": "^4.0.6",
		"@tailwindcss/postcss": "^4.0.6",
		"rehype-raw": "^7.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"tech-stack-icons": "^2.1.0",
		"unified": "^11.0.5"
	}
}
